{
  "family": "my_app",
  "executionRoleArn": "arn:aws:iam::745507191446:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "my_app_flask",
          "image": "745507191446.dkr.ecr.us-east-1.amazonaws.com/new-my-app:9f0d82ede0ddf3ddddd3d34e9676a6342a71d9d2",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8081,
              "protocol": "tcp",
              "containerPort": 8081
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/my_app",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
